        "from_node": "supply1",
        "to_node": "link1"
      },
      {
        "from_node": "link1",
        "to_node": "demand1"
      }
    ],
    "parameters": [
      {
        "name": "demand",
        "type": "Constant",
        "value": 10.0
      }
    ],
    "metric_sets": [
      {
        "name": "nodes",
        "aggregator": {
