            }
          }
        },
        "metrics": [
          {
            "type": "Node",
            "name": "demand1"
          }
        ]
      }
    ],
    "outputs": [
      {
        "name": "outputs",
        "type": "Memory",
        "metric_set": "nodes",
        "aggregation": {
          "time": {
            "type": "CountNonZero"
          },
          "metrics": {
            "type": "Max"
          },
          "scenario": {
            "type": "Sum"
          }
        }
      }
    ]
