        "to_node": "River termination"
      },
      {
        "from_node": "Reservoir",
        "from_slot": {
          "type": "Compensation"
        },
        "to_node": "River termination"
      },
      {
        "from_node": "Reservoir",
        "to_node": "Demand"
      }
    ],
    "metric_sets": [
      {
        "name": "nodes",
        "metrics": [
          {
            "type": "Node",
            "name": "Reservoir"
          },
          {
            "type": "Node",
            "name": "Demand"
          },
          {
            "type": "Node",
            "name": "River termination"
          }
        ]
      }
    ],
    "outputs": [
      {
        "name": "node-outputs",
        "type": "CSV",
        "format": "Long",
        "filename": "reservoir_with_river2-expected.csv",
        "metric_set": [
          "nodes"
        ]
