        "compensation": {
          "type": "Literal",
          "value": 1.0
        },
        "spill": "LinkNode",
        "connect_compensation_to_spill": false
      },
      {
        "meta": {
          "name": "River termination"
        },
        "type": "Output"
      },
      {
        "meta": {
          "name": "Demand"
        },
        "type": "Output",
