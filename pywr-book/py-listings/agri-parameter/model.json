{
  "metadata": {
    "title": "Python parameters",
    "description": "An example of using custom Python parameters.",
    "minimum_version": "0.1"
  },
  "timestepper": {
    "start": "2015-01-01",
    "end": "2025-12-31",
    "timestep": {
      "type": "Days",
      "days": 1
    }
  },
  "network": {
    "nodes": [
      {
        "meta": {
          "name": "supply1"
        },
        "type": "Input",
        "max_flow": {
          "type": "Literal",
          "value": 15.0
        }
      },
      {
        "meta": {
          "name": "link1"
        },
        "type": "Link"
      },
      {
        "meta": {
          "name": "demand1"
        },
        "type": "Output",
        "max_flow": {
          "type": "Parameter",
          "name": "crop1"
        },
        "cost": {
          "type": "Literal",
          "value": -10
        }
      }
    ],
    "edges": [
      {
        "from_node": "supply1",
        "to_node": "link1"
      },
      {
        "from_node": "link1",
        "to_node": "demand1"
      }
    ],
    "parameters": [
      {
        "meta": {
          "name": "crop1"
        },
        "type": "Python",
        "source": {
          "type": "Module",
          "module": "__main__"
        },
        "object": {
          "type": "Class",
          "class": "CropParameter"
        },
        "metrics": {
          "supplied": {
            "type": "Node",
            "name": "demand1",
            "attribute": "Inflow"
          }
        }
      }
    ],
    "metric_sets": [
      {
        "name": "parameters",
        "aggregator": {
          "freq": {
            "type": "Annual"
          },
          "func": {
            "type": "Sum"
          }
        },
        "metrics": [
          {
            "type": "Parameter",
            "name": "crop1",
            "return_value": "After"
          }
        ]
      }
    ],
    "outputs": [
      {
        "name": "nodes",
        "type": "CSV",
        "format": "Long",
        "filename": "agri-results.csv",
        "metric_set": "parameters"
      }
    ]
  }
}
